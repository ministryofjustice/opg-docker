  <builders>
    <hudson.tasks.Shell>
      <command>if [ -z &quot;${APP_GIT_DOWNSTREAM_BRANCH}&quot; ]; then
  printf &quot;No downstream branch specified, nothing to do. (environmental variable: APP_GIT_DOWNSTREAM_BRANCH)\n&quot;;
  exit 0;
fi

cd opg-core-docker;

if [ -n &quot;${APP_DOCKER_SUFFIX}&quot; ]; then 
	LATEST_TAG=$(semvertag --cwd &quot;/srv/jenkins/jobs/merge-tag-downstream/workspace/opg-core-back-end&quot; latest --stage &quot;${APP_DOCKER_SUFFIX}&quot;);
else 
	LATEST_TAG=$(semvertag --cwd &quot;/srv/jenkins/jobs/merge-tag-downstream/workspace/opg-core-back-end&quot; latest);
fi

./merge-tag-into-branch.sh -r &quot;${LATEST_TAG}&quot; &quot;${APP_GIT_DOWNSTREAM_BRANCH}&quot;;</command>
    </hudson.tasks.Shell>
