# known_params for Hierarchial Pages
msgid "Show siblings to the current page?"
msgstr "¿Mostrar los hermanos de la página actual?"

msgid "Always show top-level pages?"
msgstr "¿Mostrar siempre páginas de nivel superior?"

msgid "Show the static home page?"
msgstr "¿Mostrar la página de inicio estática?"

msgid "(always shown if it has child pages)"
msgstr "(siempre se muestra si tiene páginas secundarias)"

msgid "Root page ID:"
msgstr "ID de página raíz"

msgid "Exclude pages:"
msgstr "Excluir páginas:"

msgid "List of page IDs to exclude"
msgstr "Listado de IDs de página para excluir"

msgid "Sort field:"
msgstr "Ordenar campo:"

msgid ""
"Comma-separated list: <em>post_title, menu_order, post_date, post_modified, "
"ID, post_author, post_name</em>"
msgstr ""
"ista separada por comas: <em>post_title, menu_order, post_date, "
"post_modified, ID, post_author, post_name </em>"

msgid "Sort direction:"
msgstr "Ordenar dirección:"

msgid "(default: ASC)"
msgstr "(por defecto: ASC)"

msgid "Meta Key:"
msgstr "Meta Clave:"

msgid "Meta-key Value:"
msgstr "Valor Meta clave:"

msgid "for selecting pages by custom fields"
msgstr "para la selección de páginas por campos personalizados"

msgid "Authors:"
msgstr "Autores:"

msgid "Post status:"
msgstr "Estado de la publicación:"

msgid "(default: publish)"
msgstr "(por defecto: publicar)"

msgid "Post type:"
msgstr "Tipo de mensaje:"

msgid "(default: page)"
msgstr "(por defecto: página)"

# known_params for Hierarchical Categories
msgid "Title:"
msgstr "Título:"

msgid "Show siblings to the current category?"
msgstr "¿Mostrar los hermanos a la categoría actual?"

msgid "Include:"
msgstr "Incluir:"

msgid "Comma-delimited list of category IDs, or blank for all"
msgstr ""
"Lista delimitada por comas de identificadores de categorías, o en blanco "
"para todo"

msgid "Exclude:"
msgstr "Excluir:"

msgid ""
"Enter one of: <em>name, count, term_group, slug</em> or a custom value. "
"Default: name"
msgstr ""
"Introduzca uno de: <em> name, count, term_group, slug</em> o un valor "
"personalizado. Por defecto: nombre"

msgid "Only display Categories below this ID"
msgstr "Sólo mostrar las categorías por debajo de este ID"

msgid "Hide empty categories?"
msgstr "¿Ocultar categorías vacías?:"

msgid "Show count of category entries?"
msgstr "¿Mostrar recuento de entradas de categoría?"

msgid "Custom taxonomy:"
msgstr "Taxonomía Personalizada"
